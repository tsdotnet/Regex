{
  "name": "@tsdotnet/regex",
  "version": "1.0.0",
  "description": "A zero dependency, .NET style, Regular Expression class with named matches.",
  "author": "electricessence",
  "license": "MIT",
  "types": "dist/types/Regex.d.ts",
  "main": "dist/cjs/Regex.js",
  "module": "dist/es2015/Regex.js",
  "keywords": [
    "Regular Expression",
    "Regex",
    "TypeScript",
    "matches"
  ],
  "scripts": {
    "clean": "rimraf dist/*",
    "build:types": "tsc -p tsconfig/types.json",
    "build:es2015": "tsc -p tsconfig/es2015.json",
    "build:es6": "tsc -p tsconfig/es6.json",
    "build:cjs": "tsc -p tsconfig/cjs.json",
    "build": "run-s clean build:*",
    "bump": "npm version patch",
    "format": "prettier --write \"src/**/*.ts\" \"spec/**/*.ts\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags",
    "test": "ts-node -P ./tsconfig/cjs.json ./node_modules/jasmine/bin/jasmine --config=spec/jasmine.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tsdotnet/Regex.git"
  },
  "bugs": {
    "url": "https://github.com/tsdotnet/Regex/issues"
  },
  "homepage": "https://github.com/tsdotnet/Regex#readme",
  "devDependencies": {
    "@types/jasmine": "^3.5.10",
    "jasmine": "^3.5.0",
    "np": "^6.2.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "ts-node": "^8.9.0",
    "tslint": "^6.1.2",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "tslib": "^1.11.1"
  }
}
