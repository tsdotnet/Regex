{
    "name": "@tsdotnet/regex",
    "version": "1.0.2",
    "description": "A zero dependency, .NET style, Regular Expression class with named matches.",
    "author": "electricessence",
    "license": "MIT",
    "types": "dist/Regex.d.ts",
    "main": "dist/Regex.js",
    "module": "dist/Regex.js",
    "keywords": [
        "Regular Expression",
        "Regex",
        "Regexp",
        "pattern",
        "matches",
        "pattern matches",
        "named matches"
    ],
    "scripts": {
        "clean": "rimraf dist/*",
        "build": "npm run clean && copyfiles -u 1 src/**/*.d.ts dist && tsc",
        "bump": "npm version patch",
        "lint": "eslint src/**/*.ts",
        "prepublishOnly": "npm run lint && npm run build",
        "preversion": "npm run lint",
        "postversion": "git push && git push --tags",
        "test": "ts-node -p ./spec/tsconfig.json ./node_modules/jasmine/bin/jasmine --config=spec/jasmine.json"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/tsdotnet/regex.git"
    },
    "bugs": {
        "url": "https://github.com/tsdotnet/regex/issues"
    },
    "homepage": "https://github.com/tsdotnet/regex#readme",
    "devDependencies": {
        "@types/jasmine": "^3.5.10",
        "@typescript-eslint/eslint-plugin": "^2.30.0",
        "@typescript-eslint/parser": "^2.30.0",
        "copyfiles": "^2.2.0",
        "eslint": "^6.8.0",
        "eslint-config-typescript": "^3.0.0",
        "eslint-plugin-import": "^2.20.2",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-promise": "^4.2.1",
        "jasmine": "^3.5.0",
        "rimraf": "^3.0.2",
        "ts-node": "^8.9.0",
        "typescript": "^3.8.3"
    }
}
